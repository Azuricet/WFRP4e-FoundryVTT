{
    "Compendium.wfrp4e-core.talents.1IZWRr7BYOIcqPlQ": {
        "data": {
            "description": {
                "value": "<p>Your experience, talent or training lets you more safely manipulate the Winds of Magic. Whenever you roll on a miscast table as a result of a successful Channelling Test, you may apply a -10 modifier to the table result for each level of this talent. If the result is 0 or below, either roll on the lesser miscat table with the same modifier, or the miscast is nuillifed if it was a Minor Miscast.</p>"
            }
        },
        "effects": [
            {
                "_id": "mqSUg3ORIh2YCotB",
                "flags": {
                    "wfrp4e": {
                        "effectTrigger": "rollChannellingTest",
                        "effectApplication": "actor",
                        "script": "if(args.test.result.outcome == \"success\" && (args.test.result.minormis || args.test.result.majormis || args.test.result.catastrophicmis))\n{\n    args.test.result.miscastModifier = -10 * this.item.Advances\n}",
                        "preventDuplicateEffects": true,
                        "hide": false
                    }
                },
                "changes": [],
                "disabled": false,
                "duration": {
                    "startTime": null
                },
                "icon": "modules/wfrp4e-core/icons/talents/aethyric-attunement.png",
                "label": "Aethyric Attunement",
                "tint": null,
                "transfer": true
            }
        ],
        "flags": {}
    },
